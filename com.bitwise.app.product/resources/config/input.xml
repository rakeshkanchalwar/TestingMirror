<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
  <tns:component iconPath="/icons/input.png" name="Input" shape="InputFigure">
	<tns:category>INPUT</tns:category>
	<tns:inputPort>
		<tns:numberOfPorts>0</tns:numberOfPorts>
	</tns:inputPort>
	<tns:outputPort>
		<tns:numberOfPorts>1</tns:numberOfPorts>
		<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
	</tns:outputPort>
	<tns:property dataType="string" name="name" renderer="ELTComponentNameWidget" type="USER" group="GENERAL" subGroup="NAME"/>
    <tns:property dataType="string" name="schema" renderer="ELTSchemaWidget" type="USER" group="SCHEMA" subGroup="SCHEMA"/>
    <tns:property dataType="string" name="path" renderer="ELTFilePathWidget" type="USER" group="GENERAL" subGroup="FILE_PATH"/>
    <tns:property dataType="string" name="delimiter" renderer="ELTDelimeterWidget" type="USER" group="GENERAL" subGroup="DELIMITER"/>
    <tns:property dataType="boolean" name="safe" renderer="ELTSafePropertyWidget" type="USER" group="GENERAL" subGroup="SAFE"/>
    <tns:property dataType="boolean" name="has_header" renderer="ELTHasHeaderWidget" type="USER" group="GENERAL" subGroup="HEADER"/>
    <tns:property dataType="string" name="charset" renderer="ELTCharacterSetWidget" type="USER" group="GENERAL" subGroup="CHARSET"/>
    <tns:property dataType="string" name="runtime_properties" renderer="ELTRuntimePropertiesWidget" type="USER" group="RUNTIME_PROPERTIES" subGroup="RUNTIME_PROPERTIES"/>
    <tns:property dataType="string" name="phase" renderer="ELTPhaseWidget" type="USER" group="GENERAL" subGroup="PHASE"/>
  </tns:component>
</tns:Config>
