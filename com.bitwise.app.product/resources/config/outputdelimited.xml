<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/output_palette.png" canvasIconPath="/icons/output_canvas.png"
		name="OFDelimited" nameInPalette="File Delimited" defaultNamePrefix="OFDelimited">
		<tns:category>OUTPUT</tns:category>
		<tns:port>
			<tns:portSpecification>
				<tns:typeOfPort>in</tns:typeOfPort>
				<tns:numberOfPorts>1</tns:numberOfPorts>
				<tns:sequenceOfPort>1</tns:sequenceOfPort>
				<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
				<tns:linkMandatory>true</tns:linkMandatory>
				<tns:nameOfPort>in0</tns:nameOfPort>
				<tns:labelOfPort>in0</tns:labelOfPort>
			</tns:portSpecification>
			
		</tns:port>
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER"
			group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
		
		<tns:property dataType="string" group="FIELD_SEQUENCE" subGroup="RECORD_STRUCTURE_DETAILS"
			name="schema" renderer="FIELD_SEQUENCE_WIDGET" type="USER" showAsTooltip="FALSE"
			tooltipDataType="NONE" usage="REQUIRED"/>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="path"
			renderer="FILE_PATH_WIDGET" type="USER" showAsTooltip="TRUE" tooltipDataType="LINK"
			usage="REQUIRED"/>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="delimiter"
			value="|" renderer="DELIMETER_WIDGET" type="USER" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
		
		<tns:property dataType="boolean" group="GENERAL" subGroup="CONFIGURATION" name="HAS_HEADER_WIDGET"
			renderer="HAS_HEADER_WIDGET" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL"/>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="charset"
			value="UTF-8" renderer="CHARACTER_SET_WIDGET" type="USER" showAsTooltip="FALSE"
			tooltipDataType="NONE" usage="REQUIRED"/>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="runtime_properties"
			renderer="RUNTIME_PROPERTIES_WIDGET" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL"/>
		
		<tns:property dataType="string" name="phase" value="0" renderer="PHASE_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
	</tns:component>
</tns:Config>
