<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
  <tns:component paletteIconPath="/icons/gather_palette.png" 
  				 canvasIconPath="/icons/gather_canvas.png" 
  				 name="Unionall" 
  				 nameInPalette="Union All" 
  				 defaultNamePrefix="Unionall">
	<tns:category>STRAIGHTPULL</tns:category>
	<tns:port>
		<tns:portSpecification>
		    <tns:typeOfPort>in</tns:typeOfPort>
		    <tns:numberOfPorts>1</tns:numberOfPorts>
			<tns:sequenceOfPort>1</tns:sequenceOfPort>
			<tns:allowMultipleLinks>true</tns:allowMultipleLinks>
			<tns:linkMandatory>true</tns:linkMandatory>
			<tns:nameOfPort>in0</tns:nameOfPort>
		</tns:portSpecification>
		<tns:portSpecification>
		    <tns:typeOfPort>Out</tns:typeOfPort>
		    <tns:numberOfPorts>1</tns:numberOfPorts>
			<tns:sequenceOfPort>1</tns:sequenceOfPort>
			<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
			<tns:linkMandatory>true</tns:linkMandatory>
			<tns:nameOfPort>out0</tns:nameOfPort>
		</tns:portSpecification>
	</tns:port>
	<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" 
		type="USER" group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"/>
		
	<tns:property dataType="string" name="phase" value="0" renderer="PHASE_WIDGET"
		 type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"/>
		 
	 <tns:property dataType="string" name="runtime_properties" renderer="RUNTIME_PROPERTIES_WIDGET" 
	 	type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE" />
  </tns:component>
</tns:Config>
